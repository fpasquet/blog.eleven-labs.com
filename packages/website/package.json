{
  "name": "@eleven-labs/blog-website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "ts-node": "ts-node --project ./tsconfig.node.json -r dotenv/config",
    "preinstall": "yarn ts-node bin/copyAssets",
    "start": "vite",
    "test": "jest",
    "migrate": "yarn ts-node bin/migrate",
    "generate:posts-data": "yarn ts-node bin/generatePostsData",
    "generate:authors-data": "yarn ts-node bin/generateAuthorsData",
    "generate:urls-data": "yarn ts-node bin/generateUrlsData",
    "generate:data": "yarn migrate && yarn generate:posts-data && yarn generate:authors-data && yarn generate:urls-data",
    "indexation:algolia": "yarn ts-node bin/indexationOnAlgolia",
    "build:staticRender": "vite build --ssr src/staticRender.tsx --outDir dist",
    "generate:website": "yarn ts-node bin/pagesGenerator",
    "generate:js": "rollup -c",
    "generate": "yarn generate:data && yarn build:staticRender && yarn generate:website && yarn generate:js",
    "preview-prod": "yarn generate && serve dist"
  },
  "dependencies": {
    "@eleven-labs/blog-ui": "0.0.1",
    "algoliasearch": "^4.14.2",
    "hoofd": "^1.5.2",
    "html-react-parser": "^3.0.4",
    "i18next": "^21.9.0",
    "preact": "^10.10.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^28.1.6",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "cli-progress": "^3.11.2",
    "date-fns": "^2.29.1",
    "github-directory-downloader": "^1.3.6",
    "gray-matter": "^4.0.3",
    "i18next-browser-languagedetector": "^6.1.5",
    "jest": "^28.1.3",
    "markdown-it": "^13.0.1",
    "rollup-plugin-esbuild": "^4.10.1",
    "serve": "^14.0.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "yup": "^0.32.11"
  }
}
