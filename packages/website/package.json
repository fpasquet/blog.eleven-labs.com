    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/cli-progress": "^3.11.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.6",
    "@types/markdown-it": "^12.2.3",
